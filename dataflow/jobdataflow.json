{
	"name": "jobdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "jobfile",
						"type": "DatasetReference"
					},
					"name": "jobsfile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ouputsql",
						"type": "DatasetReference"
					},
					"name": "sinkjobs"
				}
			],
			"transformations": [
				{
					"name": "alterrow"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {1} as string,",
				"          {2} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> jobsfile",
				"jobsfile alterRow() ~> alterrow",
				"alterrow sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as integer,",
				"          title as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['1'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinkjobs"
			]
		}
	}
}