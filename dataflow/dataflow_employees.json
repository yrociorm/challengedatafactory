{
	"name": "dataflow_employees",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "employees_file",
						"type": "DatasetReference"
					},
					"name": "sourceEmployees"
				},
				{
					"dataset": {
						"referenceName": "departments_table",
						"type": "DatasetReference"
					},
					"name": "sourceDepartments"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "employees_table",
						"type": "DatasetReference"
					},
					"name": "sinkEmployee"
				}
			],
			"transformations": [
				{
					"name": "filteredEmployees",
					"description": "Add expression for the filter"
				},
				{
					"name": "joinDeparments"
				},
				{
					"name": "AlterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as integer,",
				"          Column_2 as string,",
				"          Column_3 as date,",
				"          Column_4 as integer,",
				"          Column_5 as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceEmployees",
				"source(output(",
				"          id as integer,",
				"          name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sourceDepartments",
				"sourceEmployees filter(!isNull(Column_4) && !isNull(Column_5)) ~> filteredEmployees",
				"filteredEmployees, sourceDepartments join(Column_5 == id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinDeparments",
				"joinDeparments alterRow(upsertIf(Column_1>0)) ~> AlterRow1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as integer,",
				"          name as string,",
				"          hire_date as date,",
				"          job_id as integer,",
				"          department_id as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['Column_1'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id = Column_1,",
				"          name = Column_2,",
				"          hire_date = Column_3,",
				"          job_id = Column_4,",
				"          department_id = Column_5",
				"     )) ~> sinkEmployee"
			]
		}
	}
}